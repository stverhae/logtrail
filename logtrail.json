{
  "es": {
    "default_index": "bnxt",
    "allow_url_parameter": false,
    "timezone": "UTC"
  },
  "tail_interval_in_seconds": 5,
  "max_buckets": 500,
  "nested_objects" : false,
  "default_time_range_in_days" : 0,
  "fields" : {
    "mapping" : {
        "timestamp" : "@timestamp",
        "display_timestamp" : "@timestamp",
        "hostname" : "type",
        "program": "@container.servicename",
        "message": "message"
    }
  }
}
